<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!--
	Mybatis生成Mapper配置文件
	1 cmd切换至项目根目录
	2 mvn mybatis-generator:generate -Dmybatis.generator.configurationFile=src/test/resources/mybatis-generator.xml -Dmybatis.generator.overwrite=true
 -->
<generatorConfiguration>
    <classPathEntry
            location="/Users/huangshizhe/.m2/repository/mysql/mysql-connector-java/5.1.20/mysql-connector-java-5.1.20.jar" />

    <context id="DB2Tables">
        <property name="beginningDelimiter" value="`" />
        <property name="endingDelimiter" value="`" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
<commentGenerator>
    <property name="suppressAllComments" value="true"></property>
    <property name="supperssDate" value="true"/>
</commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/mydatabase?user=root" userId="root" password="a19960901">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="org.graduationdesign.entity"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis/Mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="org.graduationdesign.mappers" targetProject="src/main/java">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

        <table tableName="huangshizhe" domainObjectName="huangShiZhe" delimitIdentifiers="true">
            <generatedKey column="id" sqlStatement="MYSQL" identity="true" type="post"/>
        </table>
    </context>
</generatorConfiguration>